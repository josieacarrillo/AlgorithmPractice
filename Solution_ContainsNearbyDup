class Solution {
    public boolean containsNearbyDuplicate(int[] nums, int k) {
        for(int i =0; i < nums.length-1; i++){
            for(int j = 0; j > i; j--){
                int n = i-j;
                int abs = Math.abs(n);
                if(abs>k)return false;

            }
        }
        return true;
    }
}
