class Solution {
    public int[] plusOne(int[] digits) {
        int n = digits.length;

        if(digits.length == 1 && digits[0] == 9){
            int[] plusOne = new int[2];
            plusOne[0] = 1;
            plusOne[1] = 0;

            return plusOne;
        }

    return digits;
    }
}
