class Solution {
    public int strStr(String haystack, String needle) {

        if(needle.equals(null))return 0;

        for(int i = 0; i < haystack.length(); i++){
            for(int j = 0; j < needle.length(); j++){
            char a = haystack.charAt(i);
            char b = needle.charAt(j);

            if(a == b)
                return i;






            }
        }
         return -1;


    }
}
