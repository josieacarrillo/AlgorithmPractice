class Solution {
    public boolean isPalindrome(String s) {

        String newS = s.toLowerCase();
        int i = 0;
        int j = newS.length()-1 -i;
        while(i < newS.length() && j>i)
            if(!Character.isJavaLetterOrDigit(i))
                i++;
            if(!Character.isJavaLetterOrDigit(newS.length-1-i))
               j--;

               else{
               Character temp = newS.charAt(i);
               Character last = newS.charAt(j-i);
               temp = last;
               last = newS.charAt(i);
               }
               return true;





    }
}
